    </div>\n        <transition>\n            <div class=\"app-recent-product-loading\" v-if=\"isLoading\" key=\"is-loading\">\n                <i class=\"icon-loading\"></i>\n            </div>\n            <div v-else key=\"is-loaded\">\n                <list-products :products=\"products\" col=\"col m-b-30\" ></list-products>\n            </div>\n        </transition>\n        \n    </div>\n</template>\n\n<script>\n    import {mapActions} from 'vuex'\n    import ListProducts from '../partials/list-products'\n\n    export default {\n        name: \"recent-product\",\n        components: {\n            ListProducts\n        },\n        props :{\n            numberOfRender : {\n                type : Number,\n                default : 5\n            }\n        },\n        mounted: function () {\n            let ids = this.getLocalStore('lr')\n            if (ids.length >= this.numberOfRender)  {\n                let temp = []\n                for (let i = 0; i < this.numberOfRender; i++) {\n                    temp.push(ids.pop())\n                }\n                ids = temp\n            }\n            this.isLoading = true\n            this.getRecentProduct(ids).then( (res) => {\n                let { status , data} = res.data\n                this.isLoading = false\n                if( status ){\n                    this.products = data\n                }\n                \n            })\n        },\n        data() {\n            return {\n                products: [],\n                isLoading : false,\n            }\n        },\n        methods: {\n            ...mapActions({\n                getRecentProduct: \"$_Home/GET_RECENT_PRODUCT\"\n            }),\n        }\n    }\n</script>\n\n<style lang=\"scss\" scoped>\n    @import 'src/assets/style';\n    .app-recent-product{\n        &-loading{\n            .icon-loading{\n                background: $yellow;\n            }\n        }\n    }\n</style>\n"]}]}
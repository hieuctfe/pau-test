pings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"layout.vue","sourceRoot":"src/modules/home/_components","sourcesContent":["<template>\n    <section class=\"app-home\">\n        <div class=\"app-home-page\">\n            <transition :css=\"false\" @before-enter=\"fadeBeforeEnter\" @enter=\"fadeEnter\" @leave=\"fadeLeave\" mode=\"out-in\">\n                <div class=\"container\" v-if=\"!isLoading\">\n                    <div class=\"row\">\n                        <transition name=\"slide\" mode=\"out-in\">\n                            <div class=\"col col-sidebar\" v-if=\"hasAside\">\n                                <div class=\"app-home-sidebar\">\n                                    <app-sidebar></app-sidebar>\n                                </div>\n                            </div>\n                        </transition>\n                        <div class=\"col col-content\">\n                            <transition :css=\"false\" @before-enter=\"fadeBeforeEnter\" @enter=\"fadeEnter\" @leave=\"fadeLeave\" mode=\"out-in\">\n                                <router-view />\n                            </transition>\n                        </div>\n                    </div>\n                    <div class=\"row\" v-if=\"$route.name == 'Home'\">\n                        <div class=\"col col-12\">\n                            <h1>Home 1</h1>\n                        </div>\n                        <div class=\"col col-12\">\n                            <h1>Home 2</h1>\n                        </div>\n                    </div>\n                    <div class=\"row\" v-if=\"$route.name == 'Collection' || $route.name == 'Collections'\">\n                        <div class=\"col col-12\">\n                            <h1>collection 1</h1>\n                        </div>\n                        <div class=\"col col-12\">\n                            <h1>collection 2</h1>\n                        </div>\n                    </div>\n                </div>\n            </transition>\n        </div>\n    </section>\n</template>\n<script>\n    import AppSidebar from './partials/sidebar'\n    export default {\n        name: 'HomeLayout',\n        components: {\n            AppSidebar\n        },\n        data: () => {\n            return {\n                isLoading: false,\n                noSidebarList : ['ProductDetail']\n            }\n        },\n        methods: {\n\n        },\n        computed:{\n            hasAside(){\n                return this.noSidebarList.indexOf(this.$route.name) == -1\n            }\n        },\n        created: async function () {\n            await this.$store.dispatch('$_Home/GET_LIST_COUNTRIES', {})\n            await this.$store.dispatch('$_Home/GET_LIST_SUPPLIERS', {})\n        }\n    }\n\n</script>\n<style lang=\"scss\">\n    .app-home-page {\n        padding: 30px 0;\n        .col-sidebar {\n            max-width: 270px;\n        }\n        .btn-load-more{\n            height: 44px;\n            padding: 0 25px;\n            display: inline-flex;\n            justify-content: center;\n            align-items: center;\n            border: 1px solid #ddd;\n            border-radius: 22px;\n            min-width: 150px;\n        }\n    }\n    .slide-leave-active,\n    .slide-enter-active {\n        transition: 0.5s;\n    }\n    .slide-enter {\n        transform: translate(-100%, 0);\n        opacity: 1;\n    }\n    .slide-leave-to {\n        transform: translate(-100%, 0);\n        opacity: 0;\n    }\n</style>\n"]}]}
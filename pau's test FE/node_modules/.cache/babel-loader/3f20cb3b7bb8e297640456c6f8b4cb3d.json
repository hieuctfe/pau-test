"sidebar.more || index < 10\">\n                                    <div class=\"item\">\n                                        <router-link class=\"item-name\" @click=\"reset()\" :title=\"item.title\" :to=\"setRouterLink(item)\">\n                                            {{item.title}}\n                                        </router-link>\n                                        <div class=\"item-toggle\" :class=\"{'is-active' :  sidebar.collection_id == item.id}\" v-if=\"item.childs.length\" @click.stop.prevent=\"showCollection(item)\">\n                                            <i class=\"fal fa-angle-right\"></i>\n                                        </div>\n                                    </div>\n                                    <transition :css=\"false\" @before-enter=\"fadeBeforeEnter\" @enter=\"slideDownEnter\" @leave=\"slideUpLeave\" mode=\"out-in\">\n                                        <ul class=\"sub-collections\" v-if=\"item.childs.length && sidebar.collection_id == item.id\" >\n                                            <li  v-for=\"(child , index_child) in item.childs\" :key=\"`child${index_child}`\">\n                                                <div class=\"item\">\n                                                    <router-link class=\"item-name\" @click=\"reset()\"  :title=\"item.title\" :to=\"setRouterLink(child)\">\n                                                        {{child.title}}\n                                                    </router-link>\n                                                </div>\n                                            </li>\n                                        </ul>\n                                    </transition>\n                                </li>\n                            </template>\n                        </transition-group>\n                        <li v-if=\"collections.length >= 10\">\n                            <div class=\"item-readmore\" @click=\"sidebar.more = !sidebar.more\">\n                                <button class=\"btn btn-light btn-sm\">\n                                    <template v-if=\"sidebar.more\">\n                                        Collapse\n                                    </template>\n                                    <template v-else>\n                                        Expand\n                                    </template>\n                                </button>\n                            </div>\n                        </li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"app-sidebar-box\" v-else>\n                <template v-if=\"list\">\n                    <div class=\"sidebar-title\" v-if=\"list.hasChild\">\n                        <template v-if=\"list.selected\">\n                            <strong>{{list.selected}}</strong>\n                        </template>\n                        <template v-else>\n                            <strong> All categories</strong>\n                        </template>\n                    </div>\n                    <div class=\"sidebar-collections\" >\n                        <ul class=\"collections\">\n                            <transition-group  name=\"fade\" mode=\"out-in\" >\n                                <li v-for=\"(item , index) in list.list\"  :key=\"`item${index}`\" >\n                                    <div class=\"item\">\n                                        <router-link class=\"item-name\"  :title=\"item.title\" :to=\"setRouterLink(item)\">\n                                            {{item.title}}\n                                        </router-link>\n                                        <div class=\"item-toggle\" v-if=\"item.childs\">\n                                            <i class=\"fal fa-angle-right\"></i>\n                                        </div>\n                                    </div>\n                                </li>\n                            </transition-group>\n                        </ul>\n                    </div>\n                </template>\n            </div>\n            <!-- End Box Collections -->\n            <!-- Box Supplier -->\n            <div class=\"app-sidebar-box\">\n                <div class=\"sidebar-title\">\n                    <strong>Select supplier</strong>\n                </div>\n                <div class=\"sidebar-suppliers\">\n                    <div class=\"form-group-select\">\n                        <select  @change=\"onChangeHanler('supplier_id')\"  class=\"form-control\" v-model=\"option.supplier_id\">\n                            <option value=\"\">All Suppliers</option>\n                            <option v-for=\"(item , index) in suppliers\" :key=\"index\" :value=\"item.id\">{{item.shop_name}}</option>\n                        </select>\n                    </div>\n                </div>\n            </div><!-- End Box Supplier -->\n            <!-- Box Price -->\n            <div class=\"app-sidebar-box\">\n                <div class=\"sidebar-title\">\n                    <strong>Price <small>(USD)</small></strong>\n                </div>\n                <div class=\"sidebar-price\">\n                    <div class=\"sidebar-price-content\">\n                        <div class=\"sidebar-price-input\">\n                            <money placeholder=\"From\" :allow-blank=\"true\"  v-model=\"option.min_price\" v-bind=\"money\" :min=\"0\"  class=\"form-control\" />\n                        </div>\n                        <div class=\"sidebar-price-input\">\n                            <money placeholder=\"To\" :allow-blank=\"true\" v-model=\"option.max_price\" v-bind=\"money\" :min=\"0\"  class=\"form-control\" />\n                        </div>\n                    </div>\n                    <div class=\"sidebar-price-action\">\n                        <button class=\"btn btn-sm btn-warning\" @click.stop.prevent=\"handleChangePrice()\">Apply</button>\n                    </div>\n                </div>\n            </div><!-- End Box Price -->\n            <!-- Box Shipping -->\n            <div class=\"app-sidebar-box\">\n                <div class=\"sidebar-title\">\n                    <strong>Shipping</strong>\n                </div>\n                <div class=\"sidebar-shipping\">\n                    <div class=\"sidebar-shipping-content\">\n                        <div class=\"sidebar-shipping-input\">\n                            <label for=\"sidebar_shipping_from\">From</label>\n                            <select @change=\"onChangeHanler('shipping_from')\" v-model=\"option.shipping_from\" id=\"sidebar_shipping_from\">\n                                <option value=\"\">Select shipping</option>\n                                <template v-for=\"(item , index)  in countries\">\n                                    <option v-if=\"item.code != option.shipping_to\" :key=\"`shipping${index}`\" :value=\"item.code\">{{item.name}}</option>\n                                </template>\n                            </select>\n                        </div>\n                        <div class=\"sidebar-shipping-input\">\n                            <label for=\"sidebar_shipping_to\">To</label>\n                            <select @change=\"onChangeHanler('shipping_to')\" v-model=\"option.shipping_to\" id=\"sidebar_shipping_to\">\n                                <option value=\"\">Select shipping</option>\n                                <template v-for=\"(item , index)  in countries\">\n                                    <option v-if=\"item.code != option.shipping_from\" :key=\"`shipping${index}`\" :value=\"item.code\">{{item.name}}</option>\n                                </template>\n                            </select>\n                        </div>\n                    </div>\n                </div>\n            </div><!-- End Box Shipping -->\n            <div class=\"app-sidebar-box\" v-if=\"banner\">\n                <div class=\"sidebar-images\">\n                    <a href=\"#\">\n                        <img :src=\"banner.image\" alt=\"\">\n                    </a>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n<script>\nimport { mapGetters } from 'vuex'\nimport VueSlider from 'vue-slider-component'\nimport {Money} from '@/components/v-money'\nimport helper from '@/plugins/helper'\nexport default {\n    name : 'SideBar',\n    components:{\n        VueSlider , Money\n    },\n    props:{\n        list : {}\n    },\n    data(){\n        return {\n            child_collection : '',\n            show_more : false,\n            option : {\n                supplier_id : '',\n                min_price : 0,\n                max_price : 0,\n                shipping_from : '',\n                shipping_to : ''\n            },\n            money: {\n                decimal: ',',\n                thousands: '.',\n                prefix: '',\n                suffix: ' $',\n                precision: 0,\n                masked: false \n            },\n            isLoading: false\n        }\n    },\n    computed:{\n        sidebar:{\n            get(){\n                return this.$store.getters['$_Home/getSidebarOption']\n            },\n            set(value){\n                this.$store.commit('$_Home/UPDATE_SIDEBAR_OPTION' , value)\n            }\n        },\n        banner(){\n            var banners = this.$store.getters['$_Home/getBanners']\n            if( banners.length >= 3){\n                return banners[2]\n            }\n            return null\n        },\n        ...mapGetters({\n            collections : '$_Home/getCollections',\n            suppliers : '$_Home/getSuppliers',\n            countries : '$_Home/getCountries',\n            router  : 'getRouter'\n        }),\n        \n    },\n    methods:{\n        slideDownEnter(el, done){\n            $(el).slideDown(600 ,function(){\n                done();\n            });\n        },\n        slideUpLeave(el, done){\n            $(el).slideUp(600 ,function(){\n                done();\n            });\n        },\n        setRouterLink(item){\n            var slug = helper.toSlug(item.title) + '-'+ item.id\n            var query = {}\n            if( this.$route.name == 'Collection'){\n                query = JSON.parse( JSON.stringify(this.$route.query) )\n                if( query.hasOwnProperty('page')){\n                    delete query['page']\n                }\n            }\n            var object =  {\n                name : 'Collection',\n                params :{\n                    id : slug\n                },\n                query : query\n            }\n            return this.setRouterLinkDefault(object)\n        },\n        showCollection(item){\n            this.sidebar.collection_id =  this.sidebar.collection_id == item.id ? '' : item.id\n        },\n        onChangeHanler(field){\n            var value = this.option[field] \n            this.onChange({ key : field , value : value })\n        },\n        onChange({key , value}){\n            var query = JSON.parse(JSON.stringify(this.$route.query))\n            query[key] = value\n            if( query.hasOwnProperty('page')){\n                query['page'] = 1\n            }\n            this.pushRouterLinkDefault({name : this.$route.name , query : query})\n        },\n        handleChangePrice(){\n            var query = JSON.parse(JSON.stringify(this.$route.query))\n            query['prices'] = `${this.option.min_price}-${this.option.max_price}`\n            if( query.hasOwnProperty('page')){\n                query['page'] = 1\n            }\n            this.pushRouterLinkDefault({name : this.$route.name , query : query})\n        },\n    },\n    created : async function(){\n        var shipping_from = this.$route.query.shipping_from  ? this.$route.query.shipping_from : ''\n        if( shipping_from && shipping_from != ''){\n            var find = _.find(this.countries , { code : shipping_from})\n            if( find  == undefined ){\n                shipping_from = ''\n            }\n        }\n        var shipping_to = this.$route.query.shipping_to  ? this.$route.query.shipping_to : ''\n        if( shipping_to && shipping_to != ''){\n            var find = _.find(this.countries , { code : shipping_to})\n            if( find  == undefined ){\n                shipping_to = ''\n            }\n        }\n        var supplier_id = this.$route.query.supplier_id ? this.$route.query.supplier_id : '' \n        if( supplier_id && supplier_id != ''){\n            var find = _.find(this.suppliers , { id : supplier_id})\n            if( find  == undefined ){\n                supplier_id = ''\n            }\n        }\n        this.option.shipping_from = shipping_from\n        this.option.shipping_to = shipping_to\n        this.option.supplier_id = supplier_id\n        var price = this.$route.query.prices\n        if( price ){\n            var prices = price.split('-')\n            if( prices.length == 2){\n                var min = prices[0].trim()\n                var max = prices[1].trim()\n                if( helper.isNumeric(min) && helper.isNumeric(max) && parseInt(min) <= parseInt(max)){\n                    this.option.min_price = parseInt(min)\n                    this.option.max_price = parseInt(max)\n                }\n            }\n        }\n    },\n}\n</script>\n<style lang=\"scss\" scoped>\n    @import 'src/assets/scss/static/variables';\n    \n    $border-color:#707070;\n    $collection-color : #6E6E6E;\n    $collection-color-hover : $text-color;\n    .app-sidebar{\n        &-box{\n            width: 100%;\n            margin-bottom: 20px;\n        }\n        .sidebar{\n            &-title{\n                font-size: 16px;\n                text-transform: uppercase;\n                margin-bottom: 10px;\n                padding: 6px 15px;\n                &-item{\n                    a.item-name{\n                        color:$text-color;\n                        opacity: 0.8;\n                        &:hover{\n                            color:$yellow;\n                            text-decoration: underline;\n                        }\n                    }\n                    .item-name{\n                        color:$text-color;\n                        opacity: 0.8;\n                        font-size: 14px;\n                    }\n                }\n            }\n            &-collections{\n                ul{\n                    list-style-type: none;\n                    padding: 0;\n                    margin: 0;\n                }\n                .collections{\n                    .item{\n                        display: flex;\n                        flex-direction: row;\n                        flex-wrap: nowrap;\n                        padding: 6px 6px 6px 15px;\n                        line-height: 25px;\n                        border: 1px solid transparent;\n                        &:hover{\n                            // background-color: #e2e6ea;\n                            // border-color: #dae0e5;\n                            // .item-name{\n                            //     color:$yellow;\n                            // }\n                        }\n                        &-name{\n                            flex: auto;\n                            cursor: pointer;\n                            color:$collection-color;\n                            white-space: nowrap;\n                            overflow: hidden;\n                            text-overflow: ellipsis;\n                            text-decoration: none;\n                            &.router-link-exact-active.router-link-active{\n                                color:$collection-color-hover;\n                                font-weight: bold;\n                            }\n                            &:hover{\n                                color:$collection-color-hover;\n                            }\n                        }\n                        &-toggle{\n                            width: 25px;\n                            height: 25px;\n                            display: inline-flex;\n                            justify-content: center;\n                            align-items: center;\n                            text-align: center;\n                            cursor: pointer;\n                            border-radius: 5px;\n                            transition: all 0.3s ease;\n                            transform: none;\n                            transform-origin: center center;\n                            font-size: 20px;\n                            &.is-active{\n                                transform: rotate(90deg)\n                            }\n                            &:hover{\n                                // background: #ccc;\n                                color:$yellow;\n                            }\n                        }\n                        \n                    }\n                    .item-readmore{\n                        background-color: #e2e6ea;\n                        border-color: #dae0e5;\n                        button{\n                            width: 100%;\n                            display: block;\n                        }\n                        &:hover{\n                            color:$yellow;\n                        }\n                    }\n                    .sub-collections{\n                        padding-left: 20px;\n                    }\n                }\n            }\n            &-suppliers{\n                padding: 0 15px;\n                .form-control{\n                    border-radius: 24px;\n                    border-color: $border-color;\n                    font-size: 13px;\n                }\n            }\n            &-price{\n                padding: 0 15px;\n                &-slider{\n                    margin-bottom: 10px;\n                }\n                &-content{\n                    display: flex;\n                    flex-direction: row;\n                    flex-wrap: wrap;\n                    margin-bottom: 15px;\n                }\n                &-action{\n                    text-align: right;\n                    button{\n                        color:#fff;\n                        background: $yellow;\n\n                    }\n                }\n                &-input{\n                    flex: 1 1;\n                    max-width: 100px;\n                    margin-left: 15px;\n                    &:first-child{\n                        margin-left: 0;\n                    }\n                    .form-control{\n                        border-radius: 24px;\n                        text-align: center;\n                        border-color:$border-color;\n                        font-size: 13px;\n                    }\n                }\n            }\n            &-shipping{\n                padding: 0 15px;\n                &-content{\n\n                }\n                &-input{\n                    display: flex;\n                    flex-direction: row;\n                    flex-wrap: nowrap;\n                    border: 1px solid $border-color;\n                    margin-bottom: 15px;\n                    border-radius: 24px;\n                    height: 38px;\n                    padding: 6px 5px 6px 15px;\n                    font-size: 1rem;\n                    font-weight: 400;\n                    line-height: 1.5;\n                    color: #495057;\n                    background-color: #fff;\n                    background-clip: padding-box;\n                    transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out, -webkit-box-shadow 0.15s ease-in-out;\n                    white-space: nowrap;\n                    position: relative;\n                    &:first-child{\n                        margin-left: 0;\n                    }\n                    label{\n                        width: 50px;\n                        margin: 0;\n                        color:#C5C5C5;\n                    }\n                    select{\n                        width: calc(100% - 50px);\n                        appearance: none;\n                        border: none;\n                        color:$text-color;\n                        padding-right: 20px;\n                        overflow: hidden;\n                        text-overflow: ellipsis;\n                        white-space: nowrap;\n                        font-size: 13px;\n                        &:focus{\n                            outline: none;\n                        }\n                    }\n                    &:after{\n                        content:\"\\f107\";\n                        position:absolute;\n                        z-index:2;\n                        right: 10px;\n                        top: 50%;\n                        transform: translateY(-50%);\n                        pointer-events:none;\n                        @include fontawesome;\n                        font-weight: 300;\n                        color: #495057;\n                        font-size: 20px;\n                    }\n                }\n            }\n            &-images{\n                img{\n                    max-width: 100%;\n                }\n            }\n        }\n    }\n</style>\n"],"sourceRoot":"src/modules/home/_components/partials"}]}
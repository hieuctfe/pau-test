>\n</template>\n<script>\nexport default {\n    name : 'TabFilterRouterLink',\n    methods:{\n        setRouterLink({name , value}){\n            var query = JSON.parse(JSON.stringify(this.$route.query))\n            query[name] = value\n            return this.setRouterLinkDefault( {query : query} )\n        },\n        activeRouterLink(sort){\n            return this.$route.query.sort == sort || this.$route.query.sort == undefined && sort =='last' \n        },\n    }\n}\n</script>\n<style lang=\"scss\" scoped>\n    @import 'src/assets/style';\n    .product-filter-tab-container {\n        margin-bottom: 30px;\n        width: 100%;\n        display: flex;\n        .filter-button-container {\n            flex: 1;\n            text-align: right;\n            display: block;\n            .nav-item {\n                display: inline-flex;\n                margin-left: 2px;\n                margin-right: 2px;\n                &.nav-item-special{\n                    a{\n                        color:$yellow;\n                        &.active{\n                            background: $yellow;\n                            color:#fff;\n                        }\n                    }\n                }\n            }\n            span , a{\n                color: $black;\n                text-transform: capitalize;\n                cursor: pointer;\n                min-width: 90px;\n                // padding: 6px 15px;\n                &:focus{\n                    box-shadow: 0 0 0 0.2rem rgba(255, 176, 17 , 0.25);\n                }\n                &.active {\n                    border: 1px solid $yellow;\n                }\n            }\n        }\n    }\n</style>\n"]}]}
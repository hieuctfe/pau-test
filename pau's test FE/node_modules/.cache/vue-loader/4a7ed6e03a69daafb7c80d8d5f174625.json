ents","sourcesContent":["<template>\n    <div></div>\n    <!-- <md-dialog :md-active.sync=\"showDialog\" class=\"md-dialog-confirm\" >\n        <div v-on-clickaway=\"clickaway\">\n            <div class=\"md-dialog-content p-0\">\n                <div class=\"pd-20\">\n                    <md-button class=\"close\" @click.stop.prevent=\"handleClose()\">\n                        <md-icon>close</md-icon>\n                    </md-button>\n                    <div class=\"row\">\n                        <div class=\"col col-12 m-b-10\" v-if=\"icon\">\n                            <div class=\"md-dialog-icon\" :class=\"iconClass\">\n                                <md-icon>\n                                    {{icon}}\n                                </md-icon>\n                            </div>\n                        </div>\n                        <div class=\"col col-12 m-b-5\" v-if=\"title\">\n                            <div class=\"confirm-title\" >\n                                {{title}}\n                            </div>\n                        </div>\n                        <div class=\"col col-12 m-b-5\">\n                            <div class=\"confirm-content\">\n                                {{content}}\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                \n            </div>\n            <md-dialog-actions>\n                <template v-if=\"showClose\">\n                    <md-button :class=\"closeClass\" @click.stop.prevent=\"handleClose()\">\n                        {{close}}\n                    </md-button>\n                </template>\n                <template v-if=\"save\">\n                    <md-button v-if=\"!saveLoading\" :class=\"saveClass\" @click.stop.prevent=\"handleSave()\">\n                        {{save}}\n                    </md-button>\n                    <md-button v-else :class=\"saveClass\">\n                        <svg version=\"1.1\" \n                            class=\"svg-loader\" \n                            xmlns=\"http://www.w3.org/2000/svg\" \n                            xmlns:xlink=\"http://www.w3.org/1999/xlink\" \n                            x=\"0px\" \n                            y=\"0px\"\n                            viewBox=\"0 0 80 80\" \n                            xml:space=\"preserve\">\n\n                            <path\n                                class=\"spinner\" \n                                d=\"M40,72C22.4,72,8,57.6,8,40C8,22.4,\n                                22.4,8,40,8c17.6,0,32,14.4,32,32c0,1.1-0.9,2-2,2\n                                s-2-0.9-2-2c0-15.4-12.6-28-28-28S12,24.6,12,40s12.6,\n                                28,28,28c1.1,0,2,0.9,2,2S41.1,72,40,72z\"\n                            >\n\n                                <animateTransform\n                                    attributeType=\"xml\"\n                                    attributeName=\"transform\"\n                                    type=\"rotate\"\n                                    from=\"0 40 40\"\n                                    to=\"360 40 40\"\n                                    dur=\"0.6s\"\n                                    repeatCount=\"indefinite\"\n                                />\n                            </path>\n                        </svg>\n                    </md-button>\n                    \n                </template>\n                <template v-if=\"save2\">\n                    <md-button v-if=\"!save2Loading\" :class=\"save2Class\" @click.stop.prevent=\"handleSave2()\">\n                        {{save2}}\n                    </md-button>\n                    <md-button v-else class=\"md-button-spinner\">\n                        <svg version=\"1.1\" \n                            class=\"svg-loader\" \n                            xmlns=\"http://www.w3.org/2000/svg\" \n                            xmlns:xlink=\"http://www.w3.org/1999/xlink\" \n                            x=\"0px\" \n                            y=\"0px\"\n                            viewBox=\"0 0 80 80\" \n                            xml:space=\"preserve\">\n\n                            <path\n                                class=\"spinner\" \n                                d=\"M40,72C22.4,72,8,57.6,8,40C8,22.4,\n                                22.4,8,40,8c17.6,0,32,14.4,32,32c0,1.1-0.9,2-2,2\n                                s-2-0.9-2-2c0-15.4-12.6-28-28-28S12,24.6,12,40s12.6,\n                                28,28,28c1.1,0,2,0.9,2,2S41.1,72,40,72z\"\n                            >\n\n                                <animateTransform\n                                    attributeType=\"xml\"\n                                    attributeName=\"transform\"\n                                    type=\"rotate\"\n                                    from=\"0 40 40\"\n                                    to=\"360 40 40\"\n                                    dur=\"0.6s\"\n                                    repeatCount=\"indefinite\"\n                                />\n                            </path>\n                        </svg>\n                    </md-button>\n                </template>\n            </md-dialog-actions>\n        </div>\n    </md-dialog> -->\n</template>\n<script>\nexport default {\n    name : 'ConfirmDialog',\n    props:{\n\n    },\n    data(){\n        return {\n            showDialog  : false,\n            icon        : null,\n            iconClass   : null,\n            title       : null,\n            content     : null,\n            close       : null,\n            closeClass  : null,\n            showClose   : null,\n            save        : null,\n            saveClass   : null,\n            onClose     : null,\n            onSave      : null,\n            save2       : null,\n            onSave2     : null,\n            saveLoading : false,\n            save2Loading: false\n        }\n    },\n    watch :{\n        showDialog : function(value){\n            if( value == false){\n                if( typeof this.onClose == 'function'){\n                    this.onClose();\n                }\n                \n                this.icon       = null\n                this.iconClass  = null\n                this.title      = null\n                this.content    = null\n                this.close      = null\n                this.closeClass = null\n                this.showClose  = null\n                this.save       = null\n                this.saveClass  = null\n                this.onClose    = null\n                this.onSave     = null\n                this.save2      = null\n                this.save2Class = null\n                this.onSave2    = null\n                this.saveLoading  =  false\n                this.save2Loading =  false\n            }\n        }\n    },\n    methods:{\n        show({ \n                icon= null ,\n                iconClass = null, \n                title = null, \n                content = null, \n                close = null, \n                closeClass = null,\n                showClose = true, \n                save = null, \n                saveClass = null, \n                onClose = null, \n                onSave = null,\n                onSave2 = null,\n                save2 = null,\n                save2Class = null\n            }){\n            this.icon       = icon\n            this.iconClass  = iconClass\n            this.title      = title\n            this.content    = content\n            this.close      = close\n            this.closeClass = closeClass\n            this.showClose  = showClose\n            this.save       = save\n            this.saveClass  = saveClass\n            this.onClose    = onClose\n            this.onSave     = onSave\n            this.save2      = save2\n            this.save2Class = save2Class\n            this.onSave2    = onSave2\n            this.saveLoading = false\n            this.save2Loading = false\n            this.$nextTick(()=>{\n                this.showDialog =  true\n            })\n        },\n        handleClose(){\n            if( this.saveLoading || this.save2Loading) return;\n            this.showDialog = false;\n        },\n        handleSave(){\n            if( typeof this.onSave == 'function'){\n                this.saveLoading = true\n                this.onSave().then(res=>{\n                    this.showDialog = false;\n                }, error => {\n                    this.showDialog = false;\n                });\n            }else{\n                this.showDialog = false;\n            }\n           \n        },\n        handleSave2(){\n            if( typeof this.onSave2 == 'function'){\n                this.save2Loading = true\n                this.onSave2().then(res=>{\n                    this.showDialog = false;\n                }, error => {\n                    this.showDialog = false;\n                });\n            }else{\n                this.showDialog = false;\n            }\n            \n        },\n        clickaway(){\n            if( this.saveLoading || this.save2Loading) return;\n            if( this.showDialog ){\n                this.showDialog = false;\n            }\n        }\n    },\n    mounted(){\n        EventBus.$on('vue-show-confirm-dialog',  (config) => {\n            this.show(config)\n        })\n    }\n}\n</script>\n<style lang=\"scss\" scoped>\n    .svg-loader{\n        height: 34px;\n    }\n</style>\n"]}]}
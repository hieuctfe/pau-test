     var query = JSON.parse(JSON.stringify(this.$route.query))\n            query['prices'] = `${this.option.min_price}-${this.option.max_price}`\n            if( query.hasOwnProperty('page')){\n                query['page'] = 1\n            }\n            this.pushRouterLinkDefault({name : this.$route.name , query : query})\n        },\n    },\n    created : async function(){\n        var shipping_from = this.$route.query.shipping_from  ? this.$route.query.shipping_from : ''\n        if( shipping_from && shipping_from != ''){\n            var find = _.find(this.countries , { code : shipping_from})\n            if( find  == undefined ){\n                shipping_from = ''\n            }\n        }\n        var shipping_to = this.$route.query.shipping_to  ? this.$route.query.shipping_to : ''\n        if( shipping_to && shipping_to != ''){\n            var find = _.find(this.countries , { code : shipping_to})\n            if( find  == undefined ){\n                shipping_to = ''\n            }\n        }\n        var supplier_id = this.$route.query.supplier_id ? this.$route.query.supplier_id : '' \n        if( supplier_id && supplier_id != ''){\n            var find = _.find(this.suppliers , { id : supplier_id})\n            if( find  == undefined ){\n                supplier_id = ''\n            }\n        }\n        this.option.shipping_from = shipping_from\n        this.option.shipping_to = shipping_to\n        this.option.supplier_id = supplier_id\n        var price = this.$route.query.prices\n        if( price ){\n            var prices = price.split('-')\n            if( prices.length == 2){\n                var min = prices[0].trim()\n                var max = prices[1].trim()\n                if( helper.isNumeric(min) && helper.isNumeric(max) && parseInt(min) <= parseInt(max)){\n                    this.option.min_price = parseInt(min)\n                    this.option.max_price = parseInt(max)\n                }\n            }\n        }\n    },\n}\n</script
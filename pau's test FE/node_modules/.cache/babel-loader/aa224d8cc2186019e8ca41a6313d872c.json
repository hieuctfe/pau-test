color: lighten($color-chambray, 1%);\n        *,\n        *::before,\n        *::after {\n            box-sizing: inherit;\n        }\n        &[disabled] {\n            opacity: 0.75;\n        }\n        &:hover {\n            background-color: lighten($color-chambray, 5%);\n        }\n        &:focus {\n            outline-width: 0;\n            box-shadow: 0 0 0 1px rgba($color-nepal, 0.5);\n        }\n        span {\n            position: absolute;\n            top: 50%;\n            left: 50%;\n            transform: translate(-50%,-50%);\n        }\n    }\n    .token {\n        margin-right: 0.2rem;\n        transform: translateX(-0.5rem);\n    }\n    .loader {\n        display: block;\n        border-radius: 50%;\n        border-style: solid;\n        border-width: 0.1rem;\n        border-top-color: $color-nepal;\n        border-right-color: rgba($color-white, 1);\n        border-left-color: rgba($color-white, 1);\n        border-bottom-color: rgba($color-white, 1);\n        animation: v-facebook-login-spin 2s linear infinite;\n    }\n    .loader {\n        height: 1.5rem;\n        /*margin-right: 0.5rem;*/\n    }\n    .token,\n    .loader {\n        opacity: 0.9;\n        width: 1.5rem;\n    }\n    @keyframes v-facebook-login-spin {\n        0% {\n            transform: rotate(0deg);\n        }\n        100% {\n            transform: rotate(360deg);\n        }\n    }\n</style>\n"],"sourceRoot":"src/modules/seller/_components/partials"}]}
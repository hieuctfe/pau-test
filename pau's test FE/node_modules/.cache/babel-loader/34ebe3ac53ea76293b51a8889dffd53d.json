                           <div class=\"item-flag-container\">\n                                                <span  :class=\"`flag-icon flag-icon-${item.code}`\"></span>\n                                            </div>\n                                        </div>\n                                        <span class=\"text\">{{item.name}}</span>\n                                    </li>\n                                </template>\n                            </ul>\n                        </div>\n                    </transition>\n                </div>\n            </template>\n            <template v-else>\n                <template v-for=\"(item , index) in value\">\n                    <div class=\"item-flag\" :key=\"index\" >\n                        <div class=\"item-flag-container\" >\n                            <span  :class=\"`flag-icon flag-icon-${item.code}`\"></span>\n                        </div>\n                    </div>\n                </template>\n            </template>\n        </template>\n    </div>\n</template>\n<script>\nexport default {\n    name :  'ListFlags',\n    props :{\n        value : {\n            type : Array,\n            required: true,\n        },\n    },\n    data(){\n        return {\n            showmore : false,\n            limit : 3,\n            loading  : true,\n        }\n    },\n    mounted(){\n        setTimeout( () => {\n            var container = this.$el.offsetWidth - 40;\n            var total = Math.floor(container / 39)\n            this.limit = total \n            this.loading = false\n        }, 50);\n    },\n    methods:{\n        clickOutSideShowMore(){\n            if( this.showmore ){\n                this.showmore = false;\n            }\n        },\n        toggleShowMore(){\n            this.showmore = true;\n        },\n    }\n}\n</script>\n<style lang=\"scss\" scoped>\n    @import 'src/assets/style';\n    .list-flags{\n        display: flex;\n        flex-direction: row;\n        flex-wrap: nowrap;\n\n        .item-flag{\n            &-container{\n                position: relative;\n                margin-right: 5px;\n                &:hover{\n                    .btn-remove-flag{\n                        opacity: 1;\n                        visibility: visible;\n                    }\n                }\n            }\n            .flag-icon{\n                position: relative;\n                cursor: pointer;\n            }\n            &.item-flag-more{\n                \n                position: relative;\n                .btn-show-more{\n                    width: 100%;\n                    background: $purple;\n                    color:#fff;\n                    text-align: center;\n                    border-radius: 3px;\n                    font-size: 13px;\n                    width: 34px;\n                    height: 25.5px;\n                    line-height: 25px;\n                    user-select: none;\n                    cursor: pointer;\n                }\n                .show-more-dropdown{\n                    position: absolute;\n                    left: 100%;\n                    top: 0;\n                    z-index: 10;\n                    background: #fff;\n                    box-shadow: 1px 3px 7px rgba(0, 0, 0, 0.2);\n                    border-radius: 5px;\n                    overflow: hidden;\n                    max-height: 200px;\n                    overflow: hidden auto;\n                    padding: 10px ;\n                    width:200px;\n                    padding:10px;\n                    border: 1px solid #eee;\n                    ul{\n                        list-style: none;\n                        padding: 0;\n                        margin: 0;\n                        li{\n                            margin-bottom: 5px;\n                            .text{\n                                line-height: 25px;\n                                display: block;\n                                float: left;\n                                width: calc(100% - 40px);\n                                overflow: hidden;\n                                text-overflow: ellipsis;\n                                white-space: nowrap;\n                                padding-left: 10px;\n                            }\n                            span{\n                                vertical-align: middle;\n                            }\n                            .item-flag{\n                                margin: 0;\n                                float: left;\n                                padding: 0;\n                                width: 34px;\n                                height: 25.5px;\n                                .flag-icon{\n                                    width: 34px;\n                                    height: 25.5px;\n                                }\n                            }\n                            @include clearfix;\n                            &:last-child{\n                                margin-bottom: 0;\n                            }\n                        }\n                    }\n                }\n            }\n        }\n    }\n</style>\n"],"sourceRoot":"src/modules/home/_components/partials"}]}
     </li>\n                                <li >\n                                    <div class=\"user-profile\" v-if=\"isLogged\" key=\"is-logged\">\n                                        <div class=\"dropdown\" v-if=\"seller\">\n                                            <button class=\"user-info\" type=\"button\" data-toggle=\"dropdown\">\n                                                <span class=\"user-media\">\n                                                    <img src=\"@/assets/images/default/avatar.png\" alt=\"\">\n                                                </span>\n                                                <span class=\"user-name\">\n                                                    {{seller.email}}\n                                                </span>\n                                            </button>\n                                            <div class=\"dropdown-menu dropdown-menu-right user-dropdown\" >\n                                                <ul>\n                                                    <li>\n                                                        <router-link :to=\"{name : 'Seller'}\">\n                                                            Account Info\n                                                        </router-link>\n                                                    </li>\n                                                    <li>\n                                                        <a @click=\"logout()\">Logout</a>\n                                                    </li>\n                                                </ul>\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <router-link :to=\"{name : 'Login' }\"  v-else key=\"not-login\">\n                                        Login/Register\n                                    </router-link>\n                                </li>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </section>\n</template>\n<script>\nimport FormSearch from './search'\nimport { mapGetters , mapActions } from 'vuex';\nexport default {\n    name : 'Header',\n    components:{\n        FormSearch\n    },\n    computed :{\n        ...mapGetters({\n            isLogged : '$_Seller/isLoggedIn',\n            seller : '$_Seller/getSeller',\n            router : 'getRouter'\n        })\n    },\n    methods:{\n        ...mapActions({\n            getUserInfo : '$_Seller/GET_USER_INFO'\n        }),\n        logout(){\n            this.$store.dispatch('$_Seller/LOGOUT')\n            $notify({\n                content : 'Logout successfully',\n                type : 'success'\n            })\n            if( this.router.matched.some(record => record.meta.requiresAuth) ){\n                this.redirect({name : 'Login'})\n            }\n        }\n    },\n    watch :{\n        'isLogged' : function(value){\n            if( value == true){\n                this.getUserInfo()\n            }\n        }\n    },\n    mounted(){\n        if( this.isLogged  && this.seller == null ){\n            this.getUserInfo()\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n    @import 'src/assets/style';\n    .app-header{\n        &-container{\n            background: $black;\n            width: 100%;\n            // min-height: 100px;\n            color:#fff;\n            &-top{\n                width: 100%;\n                min-height: 30px;\n                background: $grey;\n                ul{\n                    padding: 0;\n                    margin: 0;\n                    li{\n                        display: inline-block;\n                        a{\n                            display: inline-block;\n                            padding: 6px 20px;\n                            text-decoration: none;\n                            color:#fff;\n                            &:hover{\n                                color:$yellow;\n                            }\n                        }\n                    }\n                }\n            }\n            &-main{\n                background: transparent;\n                width: 100%;\n                padding: 15px 0;\n            }\n        }\n        &-logo{\n            display: inline-block;\n            padding: 0px 15px;\n            img{\n                max-width: 100%;\n                max-height: 60px;\n            }\n        }\n        &-seller{\n            text-align: right;\n            padding-top: 20px;\n            >ul{\n                padding: 0;\n                margin: 0;\n                list-style-type: none;\n                display: flex;\n                align-items: center;\n                justify-content: flex-end;\n                >li{\n                    display: inline-block;\n                    margin-left: 10px;\n                    max-width: 200px;\n                    &:first-child{\n                        margin-left: 0;\n                    }\n                    a{\n                        display: inline-block;\n                        color:#fff;\n                        padding: 6px 15px;\n                        &:hover{\n                            color:$yellow;\n                            text-decoration: none;\n                        }\n                    }\n                    .user{\n                        &-profile{\n\n                        }\n                        &-info{\n                            display: flex;\n                            flex-direction: row;\n                            flex-wrap: nowrap;\n                            align-items: center;\n                            border-radius: 0;\n                            border: none;\n                            background: transparent;\n                            color:#fff;\n                            outline: none;\n                            position: relative;\n                            &:focus,&:visited{\n                                outline: none;\n                                box-shadow: none;\n                            }\n                            &:after{\n                                content : '\\f107';\n                                @include fontawesome;\n                                font-weight: 300;\n                                margin-left: 3px;\n                            }\n                        }\n                        &-media{\n                            width: 30px;\n                            height: 30px;\n                            border-radius: 50%;\n                            overflow: hidden;\n                            margin-right: 5px;\n                            display: inline-flex;\n                            justify-content: center;\n                            align-items: center;\n                            img{\n                                max-width: 100%;\n                                max-height: 100%;\n                            }\n                        }\n                        &-name{\n                            max-width: 100px;\n                            overflow: hidden;\n                            text-overflow: ellipsis;\n                            white-space: nowrap;\n                        }\n                        &-dropdown{\n                            border-radius: 0;\n                            box-shadow: 0px 5px 5px -3px rgba(0,0,0,0.2), 0px 8px 10px 1px rgba(0,0,0,0.14), 0px 3px 14px 2px rgba(0,0,0,0.12);\n                            opacity: 0;\n                            visibility: hidden;\n                            transition: all 0.3s ease;\n                            display: block;\n                            right: 0 !important;\n                            top: 100% !important;\n                            left: auto !important;\n                            transform: translateY(10px) !important;\n                            &.show{\n                                opacity: 1;\n                                visibility: visible;\n                                transform: none !important;\n                            }\n                            ul{\n                                list-style-type: none;\n                                padding: 0;\n                                margin: 0;\n                                li{\n                                    display: block;\n                                    margin: 0;\n                                    a{\n                                        display: block;\n                                        padding: 6px 15px;\n                                        color:$text-color;\n                                        &:hover{\n                                            background: $yellow;\n                                            color:#fff;\n                                        }\n                                    }\n                                }\n                            }\n                        }\n                    }\n                }\n            }\n        }\n    }\n</style>\n"],"sourceRoot":"src/layouts/partials"}]}
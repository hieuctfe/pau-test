option value=\"\">Select shipping</option>\n                                <template v-for=\"(item , index)  in countries\">\n                                    <option v-if=\"item.code != option.shipping_from\" :key=\"`shipping${index}`\" :value=\"item.code\">{{item.name}}</option>\n                                </template>\n                            </select>\n                        </div>\n                    </div>\n                </div>\n            </div><!-- End Box Shipping -->\n            <div class=\"app-sidebar-box\" v-if=\"banner\">\n                <div class=\"sidebar-images\">\n                    <a href=\"#\">\n                        <img :src=\"banner.image\" alt=\"\">\n                    </a>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n<script>\nimport { mapGetters } from 'vuex'\nimport VueSlider from 'vue-slider-component'\nimport {Money} from '@/components/v-money'\nimport helper from '@/plugins/helper'\nexport default {\n    name : 'SideBar',\n    components:{\n        VueSlider , Money\n    },\n    props:{\n        list : {}\n    },\n    data(){\n        return {\n            child_collection : '',\n            show_more : false,\n            option : {\n                supplier_id : '',\n                min_price : 0,\n                max_price : 0,\n                shipping_from : '',\n                shipping_to : ''\n            },\n            money: {\n                decimal: ',',\n                thousands: '.',\n                prefix: '',\n                suffix: ' $',\n                precision: 0,\n                masked: false \n            },\n            isLoading: false\n        }\n    },\n    computed:{\n        sidebar:{\n            get(){\n                return this.$store.getters['$_Home/getSidebarOption']\n            },\n            set(value){\n                this.$store.commit('$_Home/UPDATE_SIDEBAR_OPTION' , value)\n            }\n        },\n        banner(){\n  
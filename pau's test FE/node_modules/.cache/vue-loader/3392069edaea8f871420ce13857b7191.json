earch\n    },\n    computed :{\n        ...mapGetters({\n            isLogged : '$_Seller/isLoggedIn',\n            seller : '$_Seller/getSeller',\n            router : 'getRouter'\n        })\n    },\n    methods:{\n        ...mapActions({\n            getUserInfo : '$_Seller/GET_USER_INFO'\n        }),\n        logout(){\n            this.$store.dispatch('$_Seller/LOGOUT')\n            $notify({\n                content : 'Logout successfully',\n                type : 'success'\n            })\n            if( this.router.matched.some(record => record.meta.requiresAuth) ){\n                this.redirect({name : 'Login'})\n            }\n        }\n    },\n    watch :{\n        'isLogged' : function(value){\n            if( value == true){\n                this.getUserInfo()\n            }\n        }\n    },\n    mounted(){\n        if( this.isLogged  && this.seller == null ){\n            this.getUserInfo()\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n    @import 'src/assets/style';\n    .app-header{\n        &-container{\n            background: $black;\n            width: 100%;\n            // min-height: 100px;\n            color:#fff;\n            &-top{\n                width: 100%;\n                min-height: 30px;\n                background: $grey;\n                ul{\n                    padding: 0;\n                    margin: 0;\n                    li{\n                        display: inline-block;\n                        a{\n                            display: inline-block;\n                            padding: 6px 20px;\n                            text-decoration: none;\n                            color:#fff;\n                            &:hover{\n                                color:$yellow;\n                            }\n                        }\n                    }\n                }\n            }\n            &-main{\n                background: transparent;\n                width: 100%;\n                padding: 15px 0;\n            }\n        }\n        &-logo{\n            display: inline-block;\n            padding: 0px 15px;\n            img{\n                max-width: 100%;\n                max-height: 60px;\n            }\n        }\n        &-seller{\n            text-align: right;\n            padding-top: 20px;\n            >ul{\n                padding: 0;\n                margin: 0;\n                list-style-type: none;\n                display: flex;\n                align-items: center;\n                justify-content: flex-end;\n                >li{\n                    display: inline-block;\n                    margin-left: 10px;\n                    max-width: 200px;\n                    &:first-child{\n                        margin-left: 0;\n                    }\n                    a{\n                        display: inline-block;\n                        color:#fff;\n                        padding: 6px 15px;\n                        &:hover{\n                            color:$yellow;\n                            text-decoration: none;\n                        }\n                    }\n                    .user{\n                        &-profile{\n\n                        }\n                        &-info{\n                            display: flex;\n                            flex-direction: row;\n                            flex-wrap: nowrap;\n                            align-items: center;\n                            border-radius: 0;\n                            border: none;\n                            background: transparent;\n                            color:#fff;\n                            outline: none;\n                            position: relative;\n                            &:focus,&:visited{\n                                outline: none;\n                                box-shadow: none;\n                            }\n                            &:after{\n                                content : '\\f107';\n            